# Environment variables template
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/monkebar

# Google Sheets (Service Account approach)
# Create a service account in Google Cloud Console, download the JSON key,
# and paste the entire JSON content here (escaped as a single line)
GOOGLE_SERVICE_ACCOUNT_JSON='{"type":"service_account","project_id":"your-project",...}'

# Your Google Spreadsheet ID (from the URL)
# https://docs.google.com/spreadsheets/d/SPREADSHEET_ID_HERE/edit
GOOGLE_SPREADSHEET_ID=your-spreadsheet-id-here

# Sheet name (tab name in your spreadsheet)
GOOGLE_SHEET_NAME=Sheet1
